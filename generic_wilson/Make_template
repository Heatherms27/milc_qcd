#  Generic routines for Wilson fermions
#  MIMD version 6
#
#  Generic make-include file for generic_wilson codes
#  This template file defines rules and macros common to all applications
#  It is intended to be an include file for other Makefiles.
#  Don't use it by itself!
#
#  The paths are relative to the application directory.

G_WILSON_ALL =  \
    bj_to_weyl.o \
    boundary_flip.o \
    copy_site_wilson_vector.o \
    d_bicgilu_w_lean.o \
    d_bicgilu_w_lean2.o \
    d_cgilu_w_lean.o \
    d_cgilu_w_lean2.o \
    d_congrad2.o \
    d_w_meson.o \
    dslash_lean.o \
    dslash_lean2.o \
    dslash_lean_forwards.o \
    dslash_w.o \
    dslash_w2.o \
    dslash_w3.o \
    io_helpers_w.o \
    io_wb3.o \
    meson_cont.o \
    mrilu_w_or.o \
    read_prop_w_fm1.o \
    w_baryon.o \
    w_baryon_hl.o \
    w_meson.o \
    w_source.o \
    w_source_h.o \
    wilson_invert.o \
    wilson_invert_lean.o

# Must list our headers in generic_wilson_includes.h
G_WILSON_HEADERS = \
  ../include/config.h \
  ../include/complex.h \
  ../include/su3.h \
  ../include/macros.h \
  ../include/comdefs.h \
  ../include/generic.h \
  ../include/generic_quark_types.h \
  ../include/generic_wilson.h \
  lattice.h

G_WILSON_ALL_DEPEND = \
  ../generic_wilson/generic_wilson_includes.h \
  ../generic_wilson/Make_template \
  ../include/Make_template \
  ${LASTMAKE} \
  ${G_WILSON_HEADERS}

${G_WILSON_ALL}: ${G_WILSON_ALL_DEPEND}

# Rules for header files
include ../include/Make_template

bj_to_weyl.o: ../generic_wilson/bj_to_weyl.c
	${CC} -c ${CFLAGS} $<
